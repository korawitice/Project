<body>
    <div class="menubar">
        <div class="container">
            <div class="logo">
                <h1><a href="Home">ICE ACE CAMERA</a></h1>
            </div>
            <ul class="menu">
                <li>
                    <a href="Home">Home</a>
                </li>
                <li>
                    <a href="Shop">Shop</a>
                </li>
                <li>
                    <a href="contact">Contact</a>
                </li>
                <li>
                    <a href="Register">Register</a>
                </li>
                <li>
                    <a href="login">Login</a>
                </li>
            </ul>
        </div>
